{
  "name": "react-virtual-masonry",
  "type": "module",
  "version": "0.0.3",
  "author": "wnhlee <2wheeh@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/2wheeh/react-virtual-masonry.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "check-types": "tsc",
    "build": "tsup",
    "release": "pnpm publish --access public",
    "prepublish": "node scripts/prepare-package.js",
    "postpublish": "node scripts/restore-package.js"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.8",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "tsup": "^8.4.0",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": ">=16 || >=17 || >= 18 || >= 19",
    "react-dom": ">=16 || >=17 || >= 18 || >=19"
  }
}
